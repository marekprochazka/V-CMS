{
  "name": "@mark.walker/v-cms",
  "author": "Mark Walker <m.prochazka2002@gmail.com>",
  "version": "1.0.4",
  "homepage": "https://marekprochazka.github.io/v-cms/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marekprochazka/v-cms.git"
  },
  "keywords": [
    "vue",
    "cms",
    "vite"
  ],
  "private": false,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "yarn lint -- && yarn test && yarn prepack",
    "lint": "eslint . --ignore-pattern \"docs/*\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "changelog": "changelogen --bump"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "quill": "^2.0.0",
    "vitepress": "^1.1.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/eslint-config": "^0.3.6",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.0",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "changelogen": "^0.5.5",
    "eslint": "^9.0.0",
    "nuxt": "^3.11.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
