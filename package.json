{
  "name": "@mark.walker/v-cms",
  "author": "Mark Walker <m.prochazka2002@gmail.com>",
  "version": "0.1.15",
  "homepage": "https://marekprochazka.github.io/v-cms/",
    "repository": {
    "type": "git",
    "url": "git+https://github.com/marekprochazka/v-cms.git"
  },
  "keywords": [
    "vue",
    "cms",
    "vite"
  ],
  "private": false,
  "type": "module",
  "exports": {
    "./core": {
      "import": "./dist/v-cms-core.js",
      "require": "./dist/v-cms-core.umd.cjs",
      "types": "./dist/core/index.d.ts"
    },
    "./utils": {
      "import": "./dist/v-cms-utils.js",
      "require": "./dist/v-cms-utils.umd.cjs",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],

  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "unit": "vitest",
    "unit-log": "vitest --disable-console-intercept",
    "coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^41.1.0",
    "@ckeditor/ckeditor5-core": "^41.1.0",
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.2.2",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vitepress": "^1.0.0-rc.42",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  }
}
