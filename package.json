{
  "name": "@mark.walker/v-cms",
  "author": "Mark Walker <m.prochazka2002@gmail.com>",
  "main": "./dist/v-cms.umd.cjs",
  "module": "./dist/v-cms.js",
  "typesVersions": {
    "*": {
      "dist/*": [
        "*.d.ts"
      ]
    },
    "lib": {
      "dist/lib/*" : [
        "components/*.d.ts",
        "composables/*.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-cms.js",
      "require": "./dist/v-cms.umd.cjs"
    }
  },
  "files": [
    "dist",
    "src/lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marekprochazka/v-cms.git"
  },
  "keywords": [
    "vue",
    "cms",
    "vite"
  ],
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  }
}
